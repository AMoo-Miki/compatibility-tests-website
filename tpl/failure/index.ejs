<div class="box-head">
    <h2 class="breadcrumb">
        <div><small>Failures</small></div>
        <div><a href="/project/<%= key %>"><%= project %></a></div>
        <div><a href="/project/<%= key %>/-/<%= src %>"><%= src %></a></div>
        <div><a href="/failures/<%= key %>/-/<%= src %>/-/<%= version %>/<%= (security ? 1 : 0) %>">OpenSearch <%= version %> <%= (security ? 'with' : 'without') %> Security</a></div>
        <div><%= id %></div>
    </h2>
</div>
<div class="box-row">
    <div class="box box-scroll-fade">
        <div class="failure-body">
            <h6>Title:</h6>
            <p><%= title %></p>
            <div class="gap-bottom-m"><h6 class="inline-block">Source:</h6> <span><%= origin %></span></div>
            <div class="gap-bottom-m"><h6 class="inline-block">Spec:</h6> <span><%= spec %></span></div>
            <div class="gap-bottom-m"><h6 class="inline-block">Build Reference:</h6> <span><%= ref %></span></div>
            <h6>Error:</h6>
            <div class="norm failure-message gap-bottom-xl"><%= error %></div>
            <div>
                <video controls width="100%">
                    <source src="https://d3udsodshx7v8u.cloudfront.net/<%= video %>" type="video/mp4" />
                </video>
            </div>
        </div>
    </div>
    <div class="box failure-side box-scroll-fade">
        <div class="scroll-y">
            <div class="gap-bottom-m"><h6 class="inline-block">Time:</h6> <span><%= timestamp %></span></div>
            <div class="gap-bottom-m"><h6 class="inline-block">Security:</h6> <span><%= security ? 'Enabled' : 'Disabled' %></span></div>
            <div class="gap-bottom-m"><h6 class="inline-block">Platform:</h6> <span><%= platform %></span></div>
            <% for (const { name, src, version } of packages) { %>
                <div class="gap-bottom-m"><h6 class="inline-block"><%= name %>:</h6> <span><%= src %>@<%= version %></span></div>
            <% } %>
            <% for (const { name, src, version } of testPackages) { %>
                <div class="gap-bottom-m"><h6 class="inline-block"><%= name %>:</h6> <span><%= src %>@<%= version %></span></div>
            <% } %>
        </div>
    </div>
</div>